decor "#pragma SDS data copy(m1[0:64][0:64])"
decor "#pragma SDS data zero_copy(prod[0:64][0:64])"

decl m1: double[64][64];
decl m2: double[64][64];
decl prod: double[64][64];

for (let jj = 0..8) {
  for (let kk = 0..8) {
    for (let i = 0..64) {
        for (let j = 0..8) {
      for (let k = 0..8) {
        let temp_x = m1[i][kk * 8 + k];
          let mul = temp_x * m2[kk * 8 + k][jj * 8 + j];
        } combine {
          prod[i][jj * 8 + j] += mul;
        }
      }
    }
  }
}

