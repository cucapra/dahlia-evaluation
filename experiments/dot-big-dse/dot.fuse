decl m1_int: bit<32>[4096];
decl m2_int: bit<32>[4096];
decl prod: bit<64>[1];

let m1: bit<32>{1}[4096 bank 2];
let m2: bit<32>{1}[4096 bank 2];

view m1_sh =     m1[_: bank 1];
view m2_sh =     m2[_: bank 1];

{
  for (let i = 0..4096) {
    m1_sh[i] := m1_int[i];
    m2_sh[i] := m2_int[i];
  }
  ---
  for (let i = 0..4096) unroll 2 {
    let x = m1[i] * m2[i];
  } combine {
    prod[0] += x;
  }
}
