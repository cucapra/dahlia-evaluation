decor "#pragma SDS data copy(m1[0:64][0:64])"

decl m1: double[64][64 bank 4];
decl m2: double[64 bank 4][64 bank 8];
decl prod: double[64][64 bank 8];

for (let i = 0..64) {
  for (let j = 0..64) unroll 8 {
    let sum: double = 0.0;
    for (let k = 0..64) unroll 4 {
      let mult = m1[i][k] * m2[k][j];
    } combine {
      sum += mult;
    }
    prod[i][j] := sum;
  }
}
